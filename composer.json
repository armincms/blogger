{
    "name": "armincms/blogger",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "armincms/armincms": "~0.2",
        "armincms/taggable": "1.*",
        "armincms/nova-targomaan": "~0.1",
        "armincms/categorizable": "1.*", 
        "armincms/home": "~0.1",
        "zareismail/markable": "~0.1", 
        "whitecube/nova-flexible-content": "~0.1",
        "outhebox/nova-hidden-field": "~1.0" 
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Blogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Blogger\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
